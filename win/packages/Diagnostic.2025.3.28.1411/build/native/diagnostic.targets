<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="IncludeHeaders" BeforeTargets="AddExternalIncludDirectoriesToPaths">
    <PropertyGroup>
      <IncludePath>$(IncludePath);$(MSBuildThisFileDirectory)include</IncludePath>
    </PropertyGroup>
  </Target>
  <ItemGroup>
    <Content Include="$(MSBuildThisFileDirectory)../lib/native/diagnostic.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(MSBuildThisFileDirectory)../lib/native/diagnostic.lib">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(MSBuildThisFileDirectory)../lib/native/diagnostic.pdb">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>